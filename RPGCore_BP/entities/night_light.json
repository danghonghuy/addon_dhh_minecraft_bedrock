		"events": {
			"dhh:sit": {
				"queue_command": {
					"command": [
						"scriptevent dhh:dragon sit"
					]
				}
			},
			"minecraft:entity_spawned": {
				"randomize": [
					{
						"add": {
							"component_groups": [
								"dhh:wandering_ground",
								"dhh:baby",
								"dhh:pouncer"
							]
						},
						"weight": 1
					},
					{
						"add": {
							"component_groups": [
								"dhh:wandering_ground",
								"dhh:baby",
								"dhh:ruffrunner"
							]
						},
						"weight": 1
					},
					{
						"add": {
							"component_groups": [
								"dhh:wandering_ground",
								"dhh:baby",
								"dhh:dart"
							]
						},
						"weight": 1
					}
				]
			},
			"dhh:hatched": {
				"queue_command": {
					"command": "playsound block.turtle_egg.crack @a[r=15] ~~~ 1 1 1"
				},
				"randomize": [
					{
						"add": {
							"component_groups": [
								"dhh:wandering_ground",
								"dhh:hatched",
								"dhh:baby",
								"dhh:pouncer"
							]
						},
						"weight": 1
					},
					{
						"add": {
							"component_groups": [
								"dhh:wandering_ground",
								"dhh:hatched",
								"dhh:baby",
								"dhh:ruffrunner"
							]
						},
						"weight": 1
					},
					{
						"add": {
							"component_groups": [
								"dhh:wandering_ground",
								"dhh:hatched",
								"dhh:baby",
								"dhh:dart"
							]
						},
						"weight": 1
					}
				]
			},
			"dhh:trained_baby": {
				"queue_command": {
					"command": "scriptevent dhh:dragon trained"
				},
				"add": {
					"component_groups": [
						"dhh:trained"
					]
				},
				"remove": {
					"component_groups": [
						"dhh:wandering_ground"
					]
				}
			},
			"dhh:start_flying": {
				"sequence": [
					{
						"filters": {
							"test": "has_component",
							"operator": "!=",
							"value": "minecraft:is_tamed"
						},
						"add": {
							"component_groups": [
								"dhh:flying"
							]
						},
						"set_property": {
							"dhh:flying": true
						}
					}
				]
			},
			"dhh:stop_flying": {
				"sequence": [
					{
						"filters": {
							"test": "has_component",
							"operator": "!=",
							"value": "minecraft:is_tamed"
						},
						"remove": {
							"component_groups": [
								"dhh:flying"
							]
						},
						"set_property": {
							"dhh:flying": false
						}
					}
				]
			}
