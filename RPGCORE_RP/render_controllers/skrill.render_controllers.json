{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.skrill": {
			"geometry": "geometry.default",
			"materials": [
				{
					"*": "material.default"
				}
			],
			"textures": [
				"(query.is_saddled?array.saddled[query.variant]:array.base[query.variant])"
			],
			"arrays": {
				"textures": {
					"Array.base": [
						"Texture.default",
						"Texture.gray",
						"Texture.green",
						"Texture.magenta",
						"Texture.orange",
						"Texture.pink",
						"Texture.purple",
						"Texture.red"
					],
					"Array.saddled": [
						"Texture.saddled_default",
						"Texture.saddled_gray",
						"Texture.saddled_green",
						"Texture.saddled_magenta",
						"Texture.saddled_orange",
						"Texture.saddled_pink",
						"Texture.saddled_purple",
						"Texture.saddled_red"
					]
				}
			}
		}
	}
}