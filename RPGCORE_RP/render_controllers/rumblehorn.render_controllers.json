{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.rumblehorn": {
			"geometry": "geometry.default",
			"materials": [
				{
					"*": "material.default"
				}
			],
			"textures": [
				"(query.is_saddled?array.saddled[query.variant]:array.base[query.variant])"
			],
			"arrays": {
				"textures": {
					"Array.base": [
						"Texture.default",
						"Texture.blue"
					],
					"Array.saddled": [
						"Texture.default_saddled",
						"Texture.blue_saddled"
					]
				}
			}
		}
	}
}