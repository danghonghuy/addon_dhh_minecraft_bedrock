{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.timberjack.main": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						"idle"
					],
					"transitions": [
						{
							"walking": "query.is_on_ground&&query.is_moving"
						},
						{
							"flying": "!query.is_on_ground&&query.vertical_speed>0"
						},
						{
							"attacking": "query.is_delayed_attacking"
						}
					],
					"blend_transition": true
				},
				"attacking": {
					"animations": [
						"bite"
					],
					"transitions": [
						{
							"default": "!query.is_delayed_attacking"
						}
					],
					"blend_transition": true
				},
				"walking": {
					"animations": [
						"walk"
					],
					"transitions": [
						{
							"default": "!query.is_on_ground||!query.is_moving"
						}
					],
					"blend_transition": true
				},
				"flying": {
					"animations": [
						"fly"
					],
					"transitions": [
						{
							"walking": "query.is_on_ground&&query.is_moving"
						},
						{
							"gliding": "!query.is_on_ground&&query.vertical_speed<0"
						},
						{
							"default": "query.is_on_ground&&!query.is_moving"
						}
					],
					"blend_transition": true,
					"sound_effects": [
						{
							"effect": "fly"
						}
					]
				},
				"gliding": {
					"animations": [
						"glide"
					],
					"transitions": [
						{
							"walking": "query.is_on_ground&&query.is_moving"
						},
						{
							"flying": "!query.is_on_ground&&query.vertical_speed>0"
						},
						{
							"default": "query.is_on_ground&&!query.is_moving"
						}
					],
					"blend_transition": true,
					"sound_effects": [
						{
							"effect": "fly"
						}
					]
				}
			}
		}
	}
}